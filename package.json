{
  "name": "nuxt-speedkit",
  "version": "2.2.4",
  "description": "Nuxt Speedkit takes over the Lighthouse performance optimization of your generated website.",
  "license": "MIT",
  "contributors": [
    {
      "name": "Stephan Gerbeth",
      "email": "stephan.gerbeth@gmail.com"
    },
    {
      "name": "Thorn-Welf Walli",
      "email": "lammpee@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GrabarzUndPartner/nuxt-speedkit"
  },
  "scripts": {
    "build:bundle": "esbuild src/runtime/tmpl/supportedBrowserDetector.mjs --platform=node --bundle --outfile=src/supportedBrowserDetector.mjs --external:browserslist --external:argue-cli --external:easy-table --external:picocolors",
    "prepack": "npm run build:bundle && rsync -r --exclude ./src ./src/* .",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "dev": "npm run build:bundle && nuxt playground",
    "build": "npm run build:bundle && nuxt build --config-file playground/nuxt.config.js",
    "generate": "npm run build:bundle && nuxt generate --config-file playground/nuxt.config.js --target static",
    "lint": "npm run lint:css && npm run lint:es",
    "lint:es": "eslint --ext .js,.vue .",
    "lint:css": "stylelint \"(src|playground)/**/*.vue\"",
    "lint:report": "eslint --ext .js,.vue -f json-relative -o ./eslint-report.json .",
    "test": "npm run build:bundle && npm run lint && npm run vitest:run",
    "vitest:run": "vitest run",
    "vitest:dev": "vitest dev",
    "start": "nuxt start --config-file playground/nuxt.config.js --target static",
    "start:generate": "npm run generate && npm run start"
  },
  "main": "module.mjs",
  "dependencies": {
    "@nuxt/image": "0.7.1",
    "browserslist-useragent-regexp": "4.1.1",
    "deepmerge": "4.3.1",
    "defu": "6.1.4",
    "dynamic-class-list": "2.0.2",
    "pathe": "1.1.2",
    "probe-image-size": "7.2.3",
    "serialize-to-js": "3.1.2",
    "sort-css-media-queries": "2.2.0",
    "vue-lazy-hydration": "2.0.0-beta.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.23.3",
    "@commitlint/cli": "18.4.4",
    "@commitlint/config-conventional": "18.4.4",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@nuxtjs/eslint-config": "12.0.0",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "commitlint": "18.4.4",
    "core-js": "3.35.0",
    "esbuild": "0.19.11",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-json-relative": "0.1.0",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-no-unsanitized": "4.0.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "2.1.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-xss": "0.1.12",
    "finalhandler": "1.2.0",
    "get-port-please": "3.1.2",
    "husky": "8.0.3",
    "jsdom": "23.2.0",
    "lint-staged": "15.2.0",
    "nuxt": "2.17.2",
    "nuxt-webpack-optimisations": "^2.2.8",
    "pinst": "3.0.0",
    "playwright": "1.40.1",
    "postcss-functions": "4.0.2",
    "postcss-html": "1.6.0",
    "postcss-nesting": "12.0.2",
    "postcss-preset-env": "9.3.0",
    "prettier": "^3.2.0",
    "serve-static": "1.15.0",
    "stylelint": "^16.1.0",
    "stylelint-config-recess-order": "4.4.0",
    "stylelint-config-standard": "36.0.0",
    "vite": "4.3.5",
    "vitest": "0.31.0",
    "vue-headings": "1.0.0",
    "vue-svg-loader": "0.16.0",
    "webpack-bundle-analyzer": "4.10.1",
    "wicg-inert": "3.1.2"
  }
}
