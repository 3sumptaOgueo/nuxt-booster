{
  "vueCompilerOptions": {
    "target": 2.7
  },
  "compilerOptions": {
    "target": "esnext",
    "module": "es2015",
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "nuxt-speedkit/*": [
        "src/runtime/*"
      ],
      "#speedkit": [
        "src/runtime/*"
      ],
      "@/*": [
        "playground/*"
      ],
      "@@/*": [
        "./*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
